<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <rotate
            android:fromDegrees="5"
            android:pivotX="0%"
            android:pivotY="0%">
            <shape android:shape="rectangle">
                <corners android:radius="63dp" />
