<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:layout_height="@dimen/ppt_height"
    tools:layout_width="@dimen/ppt_width">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <com.easefun.polyv.businesssdk.api.common.ppt.PolyvPPTWebView
            android:id="@+id/plvlc_ppt_web_view"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:clickable="false">

        </com.easefun.polyv.businesssdk.api.common.ppt.PolyvPPTWebView>

        <com.easefun.polyv.livecloudclass.modules.media.widget.PLVLCPlaceHolderView
            android:id="@+id/plvlc_ppt_placeholder"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />
    </FrameLayout>

</FrameLayout>