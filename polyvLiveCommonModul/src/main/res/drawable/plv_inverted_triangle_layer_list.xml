<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <rotate
            android:fromDegrees="45"
            android:pivotX="135%"
            android:pivotY="15%"
            android:toDegrees="45">
            <shape android:shape="rectangle">
                <size
                    android:width="12dp"
                    android:height="12dp" />
                <solid android:color="#BF1B202D" />
            </shape>
        </rotate>
    </item>
</layer-list>